{
  "name": "a11y-teams-guides",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "rimraf test && parcel src/pug/content/**/*.pug --no-source-maps --no-cache --log-level 4 --out-dir test",
    "build": "rimraf docs && parcel build src/pug/content/**/*.pug --no-source-maps --no-cache --no-content-hash --no-minify --out-dir docs"
  },
  "author": "Tatiana Fokina",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.17",
    "postcss": "^8.2.8",
    "pug": "^3.0.2",
    "sass": "^1.32.8"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "last 5 versions",
          "not dead",
          "> 5%",
          "not ie <= 11"
        ]
      }
    }
  },
  "customDistStructure": {
    "config": {
      ".css": "css",
      ".js": "js",
      "assets": [
        ".jpg",
        ".svg",
        ".png",
        ".ico"
      ],
      "fonts": [
        ".woff",
        ".woff2"
      ]
    }
  },
  "dependencies": {
    "color-scheme": "^1.0.1"
  }
}
